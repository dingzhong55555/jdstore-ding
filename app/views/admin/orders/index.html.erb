<h2>Orders List</h2>

<table class = "table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>下单用户</th>
      <th>下单时间</th>
      <th>订单状态</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= link_to order.id, admin_order_path(order) %></td>
        <td><%= order.user.email %></td>
        <td><%= order.created_at %></td>
        <td><%= order.aasm_state %></td>
      </tr>
    <% end %>
  </tbody>
</table>
